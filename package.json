{
  "name": "janus-express-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "tsc -p .",
    "dev": "nodemon --watch \"src/*\" -e \"ts,tsx,js,json\" --exec \"yarn run build && ts-node ./bin/www\" & yarn run scss",
    "scss": "sass --watch src/styles:public/stylesheets"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "sass": "^1.48.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
